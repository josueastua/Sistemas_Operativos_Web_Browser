      <VBox fx:id="vbCont" alignment="CENTER" onMouseClicked="#accionArchivo" prefHeight="175.0" prefWidth="150.0">
         <children>
            <ImageView fx:id="imvArchivo" fitHeight="59.0" fitWidth="61.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ImageView>
            <Label fx:id="lblNombre" prefHeight="82.0" prefWidth="136.0" text="Label" wrapText="true">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
