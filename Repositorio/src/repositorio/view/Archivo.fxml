<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbContenedor" alignment="CENTER" onMouseClicked="#accionArchivo" prefHeight="175.0" prefWidth="150.0" stylesheets="@../resources/Administrador.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="repositorio.controller.ArchivoController">
   <children>
      <ImageView fx:id="imvIcono" fitHeight="110.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </ImageView>
      <Label fx:id="lblNombre" prefHeight="46.0" prefWidth="140.0" text="Label" wrapText="true">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
   </children>
</VBox>
